
{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testPathIgnorePatterns": ["<rootDir>/dist/"],
  "collectCoverage": true,
  "coverageDirectory": "<rootDir>/coverage"
}
